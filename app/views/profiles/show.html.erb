<p id="notice"><%= notice %></p>

<%= image_tag @profile.avatar.url(:medium) %>
<p>
  <strong>Company Name:</strong>
  <%= @profile.name %>
</p>
<p>
  <strong>Description:</strong>
  <%= @profile.description %>
</p>
<% if @profile.user == current_user %>
	<%= link_to 'Edit', edit_profile_path(@profile) %> |
<% end %>
<%= link_to 'Back', profiles_path %>
